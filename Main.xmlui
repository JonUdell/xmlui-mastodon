<App
  layout="horizontal"
  logo="resources/xmlui-logo.svg"
  logo-dark="resources/logo-dark.svg"
  defaultTheme="earthtone"
>

  <NavPanel>
    <NavLink label="Home" to="/" icon="home" />
    <NavLink label="Followers" to="/followers" icon="users" />
    <NavLink label="ProjectLog" to="/projectLog" icon="" />
    <NavLink label="Icons" to="/icons" icon="home" />
  </NavPanel>

  <Pages inspect="true">

    <Page url="/">
      <Home />
    </Page>

    <Page url="/followers">
      <Followers />
    </Page>

    <Page url="/projectLog">
      <Markdown data="/pages/projectLog.md" />
    </Page>

    <Page url="/icons">
      <Icons />
    </Page>

  </Pages>

  <Footer>
    <Text>
      Powered by XMLUI
    </Text>
    <ToneChangerButton />
  </Footer>

</App>